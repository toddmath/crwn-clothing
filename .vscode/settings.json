{
  /* as of 2/10/2020 */
  "css.validate": false,
  "scss.validate": false,
  "less.validate": false,
  "stylelint.enable": true,
  "stylelint.packageManager": "yarn",
  /* end of 2/10/2020 */
  "cSpell.words": ["Buton", "Camo", "Yeezy", "lqip"],
  "javascript.format.enable": false,
  "terminal.integrated.experimentalRefreshOnResume": true,
  // "terminal.integrated.rendererType": "auto",
  // "terminal.integrated.shell.osx": "/usr/local/bin/bash",
  // "typescript.disableAutomaticTypeAcquisition": true,
  // "terminal.integrated.lineHeight": 1,
  // "terminal.integrated.letterSpacing": 2.8,
  // "terminal.integrated.fontSize": 14,
  // "terminal.integrated.fontWeight": "300",
  // "formattingToggle.activateFor": [
  //   "formatOnSave"
  // ],
  "editor.formatOnSave": true,
  "prettier.bracketSpacing": true,
  "prettier.arrowParens": "avoid",
  "prettier.jsxBracketSameLine": false,
  "prettier.jsxSingleQuote": true,
  "prettier.printWidth": 80,
  "prettier.quoteProps": "as-needed",
  "prettier.semi": true,
  "prettier.singleQuote": true,
  "prettier.tabWidth": 2,
  // "prettier.useTabs": true,
  "prettier.trailingComma": "es5",
  "html-css-class-completion.includeGlobPattern": "**/*.{css,scss}",
  "eslint.enable": true,
  "formattingToggle.activateFor": ["formatOnSave", "formatOnType"],
  "editor.tabSize": 2,
  "editor.detectIndentation": true,
  "node-module-intellisense.scanFileModules": true,
  "node-module-intellisense.autoStripExtensions": [
    ".js",
    ".jsx",
    ".ts",
    ".d.ts",
    ".tsx"
  ],
  "npm-intellisense.importES6": true,
  "npm-intellisense.importQuotes": "'",
  "npm-intellisense.importLinebreak": ";\r\n",
  "npm-intellisense.importDeclarationType": "const",
  "npm-intellisense.scanDevDependencies": true,
  "npm.packageManager": "yarn",
  "eslint.packageManager": "yarn",
  "gitlens.codeLens.authors.enabled": false,
  "gitlens.codeLens.enabled": false,
  "gitlens.currentLine.enabled": false,
  "gitlens.blame.avatars": true,
  "gitlens.blame.compact": true,
  "gitlens.blame.heatmap.enabled": true,
  "gitlens.blame.heatmap.location": "left",
  "gitlens.codeLens.scopes": ["containers", "blocks"],
  "gitlens.hovers.enabled": false,
  "gitlens.mode.statusBar.enabled": true,
  "gitProjectManager.codePath": "/usr/local/bin/code",
  "gitProjectManager.ignoredFolders": ["node_modules"],
  "jest.showCoverageOnLoad": true,
  "jest.pathToJest": "./node_modules/.bin/jest",
  "jest.debugMode": true,
  "jest.runAllTestsFirst": true,
  "jest.autoEnable": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.fixAll.stylelint": true
  },
  "[html]": {
    "editor.codeActionsOnSave": {
      "source.fixAll.stylelint": false
    }
  }
}
